{
  "name": "upsurge",
  "version": "0.0.0",
  "description": "Modern S3 file uploader",
  "license": "ISC",
  "main": "lib/upsurge.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "example": "cd example && npm start && npm run server",
    "lint": "eslint src test",
    "postinstall": "babel src --out-dir lib",
    "security": "nsp check package.json",
    "start": "npm run update && npm run example && watch 'npm t && npm run lint' ./src ./test",
    "test": "babel-tape-runner test/index.js | tap-dot",
    "test:cov": "babel-node node_modules/.bin/isparta cover --report text --report html test/index.js",
    "updater": "cd example && npm un upsurge && npm i kristianroebuck/upsurge",
    "update": "updtr"
  },
  "keywords": [
    "s3",
    "upload",
    "multipart"
  ],
  "author": "Kristian Roebuck <k@roeb.uk> (https://roeb.uk)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kristianroebuck/upsurge.git"
  },
  "dependencies": {
    "istanbul": "^0.4.2",
    "redux": "^3.3.0",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-tape-runner": "^2.0.0",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "isparta": "^4.0.0",
    "nock": "^7.0.2",
    "nsp": "^2.2.0",
    "tap-dot": "^1.0.1",
    "tape": "^4.4.0",
    "updtr": "^0.1.7",
    "watch": "^0.17.1",
    "webpack": "^1.12.13"
  }
}
