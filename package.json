{
  "name": "upsurge",
  "version": "0.0.0",
  "description": "Modern S3 file uploader",
  "license": "ISC",
  "main": "lib/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "lint": "eslint src test",
    "postinstall": "babel src --out-dir lib",
    "update": "cd example && npm un upsurge && npm i kristianroebuck/upsurge",
    "test": "babel-tape-runner test/** test/**/** | tap-dot",
    "test:cov": "babel-node $(npm bin)/isparta cover $(npm bin)/babel-tape-runner test/** test/**/**"
  },
  "keywords": [
    "s3",
    "upload",
    "multipart"
  ],
  "author": "Kristian Roebuck <k@roeb.uk> (https://roeb.uk)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kristianroebuck/upsurge.git"
  },
  "dependencies": {
    "redux": "^3.0.4"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.19",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-tape-runner": "^1.3.0",
    "eslint": "^1.8.0",
    "isparta": "^3.5.3",
    "tap-dot": "^1.0.0",
    "tape": "^4.2.2",
    "webpack": "^1.12.4"
  }
}
