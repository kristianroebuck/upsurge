{
  "name": "upsurge",
  "version": "0.0.0",
  "description": "Modern S3 file uploader",
  "license": "ISC",
  "main": "lib/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "example": "cd example && npm start && npm run server",
    "lint": "eslint src test",
    "postinstall": "babel src --out-dir lib",
    "start": "npm run update && npm run example && watch 'npm t && npm run lint' ./src ./test",
    "test": "babel-tape-runner test/** test/**/** | tap-dot",
    "test:cov": "babel-node node_modules/.bin/isparta cover --report text --report html test/**",
    "update": "cd example && npm un upsurge && npm i kristianroebuck/upsurge",
    "updtr": "updtr"
  },
  "keywords": [
    "s3",
    "upload",
    "multipart"
  ],
  "author": "Kristian Roebuck <k@roeb.uk> (https://roeb.uk)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kristianroebuck/upsurge.git"
  },
  "dependencies": {
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-2": "^6.1.18",
    "babel-tape-runner": "^1.3.0",
    "coveralls": "^2.11.4",
    "eslint": "^1.10.2",
    "isparta": "^4.0.0",
    "tap-dot": "^1.0.1",
    "tape": "^4.2.2",
    "updtr": "^0.1.4",
    "watch": "^0.16.0",
    "webpack": "1.12.9"
  }
}
